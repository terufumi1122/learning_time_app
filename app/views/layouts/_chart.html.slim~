canvas#chartTimes width="400" height="400"
javascript:
  var ctx = document.getElementById('chartTimes').getContext('2d');
  var myChart = new Chart(ctx, {
      type: 'bar',
      data: {
          labels: [
              moment(new Date(gon.created_time_at_the_6th_to_last)).format("MM/DD hh:DD")
              moment(new Date(gon.created_time_at_the_5th_to_last)).format("MM/DD hh:DD"),
              moment(new Date(gon.created_time_at_the_4th_to_last)).format("MM/DD hh:DD"),
              moment(new Date(gon.created_time_at_the_3rd_to_last)).format("MM/DD hh:DD"),
              moment(new Date(gon.created_time_at_the_2nd_to_last)).format("MM/DD hh:DD"),
              moment(new Date(gon.created_time_at_the_1st_to_last)).format("MM/DD hh:DD"),
                ],
          datasets: [{
              label: '# of Votes',
              data: [
              gon.learning_time_at_the_6th_to_last,
              gon.learning_time_at_the_5th_to_last,
              gon.learning_time_at_the_4th_to_last,
              gon.learning_time_at_the_3rd_to_last,
              gon.learning_time_at_the_2nd_to_last,
              gon.learning_time_at_the_1st_to_last
              ],
              backgroundColor: [
                  'rgba(255, 99, 132, 1.0)',
                  'rgba(54, 162, 235, 1.0)',
                  'rgba(255, 206, 86, 1.0)',
                  'rgba(75, 192, 192, 1.0)',
                  'rgba(153, 102, 255, 1.0)',
                  'rgba(255, 159, 64, 1.0)'
              ],
              borderColor: [
                  'rgba(255, 99, 132, 1)',
                  'rgba(54, 162, 235, 1)',
                  'rgba(255, 206, 86, 1)',
                  'rgba(75, 192, 192, 1)',
                  'rgba(153, 102, 255, 1)',
                  'rgba(255, 159, 64, 1)'
              ],
              borderWidth: 1
          }]
      },
      options: {
          scales: {
              yAxes: [{
                  ticks: {
                      beginAtZero: true
                  }
              }]
          }
      }
  });
